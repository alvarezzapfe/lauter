{
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    }
  ],
  "installCommand": "cd backend && npm install && npm run postinstall",
  "buildCommand": "cd backend && npm run build"
}
